{
  "name": "Swifty",
  "version": "0.6.6",
  "description": "Modern, Lightweight, Fast and Free Password Manager",
  "repository": "https://github.com/alchaplinsky/swifty.git",
  "dependencies": {
    "@swiftyapp/aes-256-gcm": "^1.0.1",
    "@swiftyapp/cryptor": "^1.1.2",
    "@swiftyapp/vault": "^0.1.0",
    "classnames": "^2.3.1",
    "electron-log": "^4.4.4",
    "electron-notarize": "^1.1.1",
    "electron-unhandled": "^3.0.2",
    "electron-util": "^0.17.2",
    "fs-extra": "^10.0.0",
    "generate-password": "^1.7.0",
    "googleapis": "^89.0.0",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "mousetrap": "^1.6.5",
    "nucleon": "^1.0.20",
    "parse-domain": "^4.1.0",
    "password-validator": "^5.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "remove": "^0.1.5",
    "shortid": "^2.2.16",
    "speakeasy": "^2.0.0",
    "universal-analytics": "^0.4.23"
  },
  "scripts": {
    "lint": "npx eslint --color src",
    "start": "NODE_ENV=development bozon start",
    "test": "yarn lint && yarn test:unit && yarn test:features",
    "test:unit": "NODE_ENV=test npx jest test/units",
    "test:features": "NODE_ENV=test bozon test test/features"
  },
  "author": {
    "name": "Alex Chaplinsky",
    "email": "alchaplinsky@gmail.com"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "bozon": "^1.2.9",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "electron": "^13.5.2",
    "eslint": "^8.6.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ps-node": "^0.1.6",
    "react-svg-loader": "^3.0.3",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "spectron": "^15.0.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0"
  },
  "resolutions": {
    "js-yaml": "^3.14.1",
    "json-schema": "^0.4.0",
    "yargs-parser": "^20.2.9",
    "xmldom": "xmldom/xmldom#0.7.0"
  },
  "build": {
    "appId": "com.electron.swifty",
    "afterSign": "scripts/notarize.js",
    "fileAssociations": [
      {
        "ext": "swftx",
        "role": "None",
        "icon": "build/fileicon.icns",
        "mimeType": "application/vnd.swftx"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "alchaplinsky",
        "repo": "swifty"
      }
    ],
    "win": {
      "icon": "build/icon.ico",
      "publisherName": "Developer ID Application: Oleksandr Chaplinsky (UFBL3F444A)",
      "verifyUpdateCodeSignature": false
    },
    "linux": {
      "category": "Utility",
      "icon": "build/icon.png",
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ]
    },
    "mac": {
      "icon": "build/icon.icns",
      "category": "public.app-category.utilities",
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "target": [
        "dmg",
        "zip",
        "mas"
      ]
    }
  }
}
